{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"paths": {
			"$store": ["src/store"],
			"$components/*": ["src/components/*"],
			"$icons/*": ["src/icons/*"],
			"icons": ["node_modules/svelte-feather-icons/src/icons"]
		}
	},
	"include": ["src/**/*.d.ts", "src/**/*.js", "src/**/*.ts", "src/**/*.svelte"]
}
