{
	"name": "hyp-files-app",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "npm run ts-build && svelte-kit build",
		"ts-build": "tsc -p backend/tsconfig.json --outDir build/backend",
		"serve": "node build/backend/server.js",
		"serve:dev:host": "nodemon -e ts -w backend --exec 'npm run ts-build && npm run serve -- --host'",
		"serve:dev": "nodemon -e ts -w backend --exec 'npm run ts-build && npm run serve'",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@rollup/plugin-babel": "^5.3.0",
		"@rollup/plugin-commonjs": "^20.0.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-multi-entry": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.0.4",
		"@sveltejs/adapter-static": "^1.0.0-next.21",
		"@sveltejs/kit": "next",
		"@types/node": "^16.9.0",
		"@typescript-eslint/eslint-plugin": "^4.19.0",
		"@typescript-eslint/parser": "^4.19.0",
		"autoprefixer": "^10.2.5",
		"builtin-modules": "^3.2.0",
		"cssnano": "^5.0.1",
		"eslint": "^7.22.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-svelte3": "^3.2.0",
		"nodemon": "^2.0.13",
		"postcss": "^8.2.10",
		"postcss-load-config": "^3.0.1",
		"prettier": "~2.2.1",
		"prettier-plugin-svelte": "^2.2.0",
		"rollup": "^2.56.3",
		"rollup-plugin-hypothetical": "^2.1.1",
		"rollup-plugin-polyfill-node": "^0.7.0",
		"svelte": "^3.34.0",
		"svelte-check": "^2.0.0",
		"svelte-preprocess": "^4.7.1",
		"tailwindcss": "^2.1.1",
		"tslib": "^2.0.0",
		"typescript": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@beyonk/svelte-notifications": "^3.1.0",
		"@corestore/networker": "github:Anyass3/corestore-networker",
		"@fontsource/fira-mono": "^4.4.2",
		"@hyperswarm/dht": "^5.0.0-rc.12",
		"amd-loader": "^0.0.8",
		"archiver": "^5.3.0",
		"axios": "^0.21.1",
		"body-parser": "^1.19.0",
		"chalk": "^4.1.0",
		"colors": "^1.4.0",
		"compression": "^1.7.4",
		"connectome": "^0.0.108",
		"cors": "^2.8.5",
		"crypto": "^1.0.1",
		"express": "^4.17.1",
		"file-type": "^16.5.3",
		"hyperbee": "^1.5.5",
		"hypercore-promisifier": "^1.1.0",
		"hypercore-protocol": "^8.0.7",
		"hyperdrive": "^10.21.0",
		"hyperswarm": "^3.0.0-rc.0",
		"lodash-es": "^4.17.21",
		"markdown-hljs": "^0.1.11",
		"mime": "^1.6.0",
