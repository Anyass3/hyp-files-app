{
	"name": "hyp-files-app",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "npm run ts-build && svelte-kit build",
		"ts-build": "tsc -p backend/tsconfig.json --outDir build/backend",
		"serve": "npm run ts-build && node build/backend/server.js",
		"serve:dev": "nodemon -e ts -w backend --exec npm run ts-build && npm run serve",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^1.0.0-next.28",
		"@sveltejs/adapter-node": "^1.0.0-next.69",
		"@sveltejs/kit": "next",
		"@types/lodash-es": "^4.17.5",
		"@types/node": "^16.9.0",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"nodemon": "^2.0.13",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"prompt-sync": "^4.2.0",
		"svelte": "^3.46.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.1",
		"tailwindcss": "^3.0.7",
		"tslib": "^2.3.1",
		"typescript": "~4.5.4"
	},
	"type": "module",
	"dependencies": {
		"@beyonk/svelte-notifications": "^3.1.0",
		"@corestore/networker": "github:Anyass3/corestore-networker",
		"@fontsource/fira-mono": "^4.4.2",
		"@hyperswarm/dht": "^5.0.15",
		"archiver": "^5.3.0",
		"axios": "^0.21.1",
		"body-parser": "^1.19.0",
		"chalk": "^4.1.0",
		"colors": "^1.4.0",
		"compression": "^1.7.4",
		"connectome": "^0.1.7",
		"cors": "^2.8.5",
		"crypto": "^1.0.1",
		"express": "^4.17.1",
		"file-type": "^16.5.3",
		"hyperbee": "^1.5.5",
		"hypercore-promisifier": "^1.1.0",
		"hypercore-protocol": "^8.0.7",
		"hyperdrive": "^10.21.0",
		"hyperswarm": "^3.0.1",
		"lodash-es": "^4.17.21",
		"markdown-hljs": "^0.1.11",
		"mime": "^3.0.0",
		"pump": "^3.0.0",
		"random-words": "^1.1.1",
		"read-chunk": "^3.2.0",
		"sidebar-swipe": "^0.8.81",
		"stores-x": "^0.0.872",
		"svelte-feather-icons": "^3.5.0",
		"uuid": "^8.3.2"
	}
}